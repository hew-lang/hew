      "duration"
    ],
    "collections": ["Vec", "HashMap"],
    "option_result": ["Option", "Result", "Ok", "Err", "Some", "None"],
    "concurrency": [
      "ActorRef",
      "Stream",
      "Sink",
      "Task",
      "Scope",
      "Generator",
      "AsyncGenerator"
    ],
    "marker_traits": ["Send", "Frozen", "Copy", "Arc", "Rc", "Weak"],
    "other": ["Range", "ActorStream"]
  },
  "string_prefixes": ["f", "r", "re"],
  "comment_styles": {
    "line": "//",
    "doc": "///",
    "inner_doc": "//!",
    "block_start": "/*",
    "block_end": "*/"
  },
  "literal_suffixes": {
    "duration": ["ns", "us", "ms", "s", "m", "h"]
  }
