fn pascal(n: int) -> int {
    let row = Vec::new();
    row.push(1);
    var i = 0;
    while i < n {
        row.push(0);
        var j = row.len() - 1;
        while j > 0 {
            row.set(j, row.get(j) + row.get(j - 1));
            j = j - 1;
        }
        i = i + 1;
    }
    row.get(n / 2)
}

fn main() {
    let iters = 100000;
    var i = 0;
    var sum = 0;
    while i < iters {
        sum = sum + pascal(20);
        i = i + 1;
    }
    println(iters);
    println(sum);
}
