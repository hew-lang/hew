fn fibonacci(n: int) -> int {
    var a = 0;
    var b = 1;
    for i in 0..n {
        let temp = b;
        b = a + b;
        a = temp;
    }
    a
}

fn main() {
    let iters = 1000000;
    var sum = 0;
    for i in 0..iters {
        sum = sum + fibonacci(30);
    }
    println(iters);
    println(sum);
}
