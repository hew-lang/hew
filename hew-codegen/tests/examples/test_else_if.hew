fn main() {
    let x: int = 42;
    if x < 0 {
        println(-1);
    } else if x == 0 {
        println(0);
    } else if x < 10 {
        println(1);
    } else if x < 100 {
        println(2);
    } else {
        println(3);
    }
    let y: int = -5;
    if y < 0 {
        println(-1);
    } else if y == 0 {
        println(0);
    } else {
        println(1);
    }
    let z: int = 0;
    if z < 0 {
        println(-1);
    } else if z == 0 {
        println(0);
    } else {
        println(1);
    }
}
