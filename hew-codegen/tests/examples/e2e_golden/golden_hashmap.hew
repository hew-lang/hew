// Golden test: HashMap operations
// Exercises: HashMapNewOp, HashMapInsertOp, HashMapGetOp, HashMapContainsKeyOp,
//            HashMapRemoveOp, HashMapLenOp, HashMapFreeOp
fn main() {
    let m: HashMap<String, int> = HashMap::new();

    // insert + len
    m.insert("x", 10);
    m.insert("y", 20);
    m.insert("z", 30);
    println(m.len());

    // get
    match m.get("x") {
        Some(v) => println(v),
        None => println("none"),
    }
    match m.get("y") {
        Some(v) => println(v),
        None => println("none"),
    }
    match m.get("z") {
        Some(v) => println(v),
        None => println("none"),
    }

    // contains_key
    println(m.contains_key("x"));
    println(m.contains_key("w"));

    // overwrite
    m.insert("x", 100);
    match m.get("x") {
        Some(v) => println(v),
        None => println("none"),
    }
    println(m.len());

    // remove
    m.remove("y");
    println(m.len());
    println(m.contains_key("y"));
}
