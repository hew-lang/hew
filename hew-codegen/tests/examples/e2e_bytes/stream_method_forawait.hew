import std::fs;
import std::stream;
import std::os;

fn main() {
    let tmp = os.temp_dir();
    let path = tmp + "/hew_method_forawait.txt";
    unsafe { hew_file_write(path, "one\ntwo\nthree\n"); }
    let raw = match stream.from_file(path) {
        Ok(s) => s,
        Err(e) => { panic(e); },
    };
    let lines = raw.lines();
    var count: int = 0;
    for await line in lines {
        count = count + 1;
    }
    println(count);
}
