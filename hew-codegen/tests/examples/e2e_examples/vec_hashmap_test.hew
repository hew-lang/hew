// Vec and HashMap test
// Exercises the core collection types from Hew programs
fn test_vec() -> int {
    let v: Vec<int> = Vec::new();
    v.push(10);
    v.push(20);
    v.push(30);
    let len = v.len();
    let first = v.get(0);
    let second = v.get(1);
    let last = v.pop();
    println(first); // 10
    println(second); // 20
    println(last); // 30
    println(len); // 3

    // After pop, len should be 2
    let len2 = v.len();
    println(len2); // 2
    first + second + last
}

fn test_hashmap() -> int {
    let m: HashMap<String, int> = HashMap::new();
    m.insert("one", 1);
    m.insert("two", 2);
    m.insert("three", 3);
    let len = m.len();
    println(len); // 3
    len
}

fn main() {
    println("=== Vec Test ===");
    let vec_result = test_vec();
    println(vec_result); // 60
    println("=== HashMap Test ===");
    let map_result = test_hashmap();
    println(map_result);
} // 3
