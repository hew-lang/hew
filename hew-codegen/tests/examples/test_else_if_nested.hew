// E2E test: nested if/else-if inside else-if branches
fn main() {
    let x = 15;
    let y = 3;
    if x < 10 {
        println(1);
    } else if x < 20 {
        // Nested if inside else-if
        if y < 5 {
            println(2);
        } else {
            println(3);
        }
    } else {
        println(4);
    }
    // else-if with boolean expressions
    let a = true;
    let b = false;
    if a {
        if b {
            println(10);
        } else {
            println(20);
        }
    } else if b {
        println(30);
    } else {
        println(40);
    }
}
