trait Printable {
    fn show(self: Self);
}

type Dog {
    legs: int;
}

type Cat {
    lives: int;
}

impl Printable for Dog {
    fn show(self: Dog) {
        println(self.legs);
    }
}

impl Printable for Cat {
    fn show(self: Cat) {
        println(self.lives);
    }
}

fn display(obj: dyn Printable) {
    obj.show();
}

fn main() {
    let d = Dog { legs: 4 };
    let c = Cat { lives: 9 };
    display(d);
    display(c);
}
