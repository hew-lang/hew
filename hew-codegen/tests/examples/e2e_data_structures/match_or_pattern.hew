// Test: Or-patterns in match expressions
// Exercises: PatternKind::Or with literal alternatives
fn classify(n: int) -> int {
    match n {
        1 | 2 | 3 => 1,
        4 | 5 | 6 => 2,
        _ => 0,
    }
}

fn main() {
    println(classify(2));
    println(classify(5));
    println(classify(9));
}
