enum Color {
    Red;
    Green;
    Blue;
}

enum Shape {
    Circle(int);
    Rectangle(int);
}

fn describe_color(tag: int) -> int {
    match tag {
        0 => {
            println("Red");
            0
        },
        1 => {
            println("Green");
            1
        },
        2 => {
            println("Blue");
            2
        },
        _ => {
            println("Unknown");
            -1
        },
    }
}

fn main() {
    println("=== Enum Test ===");
    let _ = describe_color(0);
    let _ = describe_color(1);
    let _ = describe_color(2);
    let _ = describe_color(99);
}
