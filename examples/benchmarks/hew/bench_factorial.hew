fn factorial(n: int) -> int {
    var result = 1;
    var i = 1;
    while i <= n {
        result = result * i;
        i = i + 1;
    }
    result
}

fn main() {
    let iters = 1000000;
    var i = 0;
    var sum = 0;
    while i < iters {
        sum = sum + factorial(20);
        i = i + 1;
    }
    println(iters);
    println(sum);
}
