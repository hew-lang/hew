// Test: JSON serialization of all scalar wire field types
// boolâ†’i32, i32, i64, f64, String with default (Preserve) naming.
// JSON keys are alphabetically sorted by serde_json.
wire type Scalars {
    big: i64 @1;
    count: i32 @2;
    flag: bool @3;
    label: String @4;
    ratio: f64 @5;
}

extern "C" {
    fn Scalars_to_json(big: i64, count: i32, flag: i32, label: String, ratio: f64) -> String;
}

fn main() {
    let json = Scalars_to_json(1000000, -7, 1, "hello", 1.5);
    println(json);
}
