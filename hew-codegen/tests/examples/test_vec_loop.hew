fn main() {
    let v: Vec<int> = Vec::new();
    var i: int = 0;
    while i < 5 {
        v.push(i * 10);
        i = (i + 1) as int;
    }
    println(v.len());
    i = 0;
    var sum: int = 0;
    while i < v.len() {
        (sum = sum + v.get(i)) as int;
        i = (i + 1) as int;
    }
    println(sum);
    let popped = v.pop();
    println(popped);
    println(v.len());
}
