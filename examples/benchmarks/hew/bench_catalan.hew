fn catalan(n: int) -> int {
    let dp = Vec::new();
    dp.push(1);
    dp.push(1);
    for i in 2..(n + 1) {
        dp.push(0);
        for j in 0..i {
            dp.set(i, dp.get(i) + dp.get(j) * dp.get(i - 1 - j));
        }
    }
    dp.get(n)
}

fn main() {
    let iters = 500000;
    var sum = 0;
    for i in 0..iters {
        sum = sum + catalan(15);
    }
    println(iters);
    println(sum);
}
