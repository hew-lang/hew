fn is_prime(n: int) -> int {
    if n < 2 {
        return 0;
    }
    var i = 2;
    while i * i <= n {
        if n % i == 0 {
            return 0;
        }
        i = i + 1;
    }
    1
}

fn main() {
    let iters = 100000;
    var i = 0;
    var sum = 0;
    while i < iters {
        sum = sum + is_prime(104729);
        i = i + 1;
    }
    println(iters);
    println(sum);
}
