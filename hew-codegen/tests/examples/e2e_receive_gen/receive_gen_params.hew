actor Streamer {

    receive gen fn from_range(start: int, count: int) -> int {
        var i: int = 0;
        while i < count {
            yield start + i;
            i = (i + 1) as int;
        }
    }
}

fn main() {
    let s = spawn Streamer;
    for await val in s.from_range(7, 3) {
        println(val);
    }
}
