// Comprehensive async/await demo for Hew language
async fn simple_calculation(n: i32) -> i32 {
    let result = n * 2 + 1;
    result
}

async fn print_async_message(msg: i32) {
    println(msg);
}

fn main() {
    // Test async function with return value
    let future = simple_calculation(21);
    let result = await future;
    println(result);

    // Test async function without return value (direct await)
    await print_async_message(123);
    println(999);
}
