// Test: Vec get() borrowed string lifecycle
// Ensures values returned from Vec<String>.get() are treated as borrowed.
fn main() -> int {
    println("=== Vec Get Borrowed String ===");
    let v: Vec<String> = Vec::new();
    v.push("hello");
    v.push("world");
    let first: String = v.get(0);
    let second = v.get(1);
    if string_equals(first, "hello") {
        if string_equals(second, "world") {
            println("PASS");
            0
        } else {
            println("FAIL");
            1
        }
    } else {
        println("FAIL");
        1
    }
}
