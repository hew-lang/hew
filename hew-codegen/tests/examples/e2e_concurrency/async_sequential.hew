// Test: Sequential Computation
// Exercises: Function composition, sequential data flow
// NOTE: async/await has a known codegen issue; this tests the pattern synchronously
fn compute_square(n: int) -> int {
    n * n
}

fn compute_add(a: int, b: int) -> int {
    a + b
}

fn main() -> int {
    println("=== Sequential Computation ===");
    let r1 = compute_square(5);
    println(f"5^2 = {r1}");
    let r2 = compute_square(10);
    println(f"10^2 = {r2}");
    let r3 = compute_add(r1, r2);
    println(f"25 + 100 = {r3}");
    if r3 == 125 {
        println("PASS");
        0
    } else {
        println("FAIL");
        1
    }
}
