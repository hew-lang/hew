fn main() {
    let re = re"[0-9]+";
    if re.is_match("hello123world") {
        println("digit match");
    }
    if re.is_match("no digits here") {
        println("unexpected");
    } else {
        println("no digit match");
    }
    re.free();

    let re2 = re"^[a-z]+$";
    if re2.is_match("hello") {
        println("alpha match");
    }
    if re2.is_match("Hello") {
        println("unexpected");
    } else {
        println("no alpha match");
    }
    re2.free();
}
