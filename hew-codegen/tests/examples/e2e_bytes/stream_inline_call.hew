import std::fs;
import std::stream;
import std::os;

fn main() {
    let tmp = os.temp_dir();
    let path = tmp + "/hew_inline_stream_test.txt";
    unsafe { hew_file_write(path, "alpha\nbeta\ngamma\n"); }

    let raw = match stream.from_file(path) {
        Ok(s) => s,
        Err(e) => { panic(e); },
    };
    var count = 0;
    for await line in raw.lines() {
        count = count + 1;
    }
    println(count);
}
