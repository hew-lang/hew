fn pascal(n: int) -> int {
    let row = Vec::new();
    row.push(1);
    for i in 0..n {
        row.push(0);
        var j = row.len() - 1;
        while j > 0 {
            row.set(j, row.get(j) + row.get(j - 1));
            j = j - 1;
        }
    }
    row.get(n / 2)
}

const ITERS: int = 100000;

fn main() {
    var sum = 0;
    for i in 0..ITERS {
        sum = sum + pascal(20);
    }
    println(ITERS);
    println(sum);
}
