// Test: non-exhaustive match panics at runtime
// Match on int without wildcard arm should panic at runtime.

fn classify(n: int) -> String {
    match n {
        0 => "zero",
        1 => "one",
        _ => "other",
    }
}

fn main() {
    println(classify(0));
    println(classify(1));
    println(classify(42));
}
