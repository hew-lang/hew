fn counting_sort(v: Vec<int>) -> Vec<int> {
    let n = v.len();
    if n == 0 {
        return v;
    }
    var max_val = v.get(0);
    var i = 1;
    while i < n {
        if v.get(i) > max_val {
            max_val = v.get(i);
        }
        i = i + 1;
    }
    let count: Vec<int> = Vec::new();
    i = 0;
    while i <= max_val {
        count.push(0);
        i = i + 1;
    }
    i = 0;
    while i < n {
        let val = v.get(i);
        count.set(val, count.get(val) + 1);
        i = i + 1;
    }
    var pos = 0;
    i = 0;
    while i <= max_val {
        var c = count.get(i);
        while c > 0 {
            v.set(pos, i);
            pos = pos + 1;
            c = c - 1;
        }
        i = i + 1;
    }
    v
}

fn make_test_array() -> Vec<int> {
    let v: Vec<int> = Vec::new();
    var i = 100;
    while i > 0 {
        v.push(i);
        i = i - 1;
    }
    v
}

fn main() {
    let iters = 200000;
    var i = 0;
    while i < iters {
        let v = make_test_array();
        counting_sort(v);
        i = i + 1;
    }
    println(iters);
}
