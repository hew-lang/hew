// Enum with pattern matching

enum Color {
    Red;
    Green;
    Blue;
}

fn color_name(c: Color) {
    match c {
        Red => println("Red"),
        Green => println("Green"),
        Blue => println("Blue"),
    }
}

fn main() {
    let c1 = Red;
    let c2 = Green;
    let c3 = Blue;

    color_name(c1);
    color_name(c2);
    color_name(c3);
}
